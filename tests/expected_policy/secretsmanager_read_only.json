{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "readSecrets",
            "Action": [
                "secretsmanager:GetSecretValue",
                "secretsmanager:DescribeSecret",
                "secretsmanager:ListSecrets"
            ],
            "Effect": "Allow",
            "Resource": ["arn:aws:secretsmanager:*:*:secret:test_secret*"]
        }
    ]
}
